{
    "functions": {
        "api/clips.ts": {
            "maxDuration": 10
        },
        "api/clips/[id].ts": {
            "maxDuration": 10
        }
    },
    "buildCommand": "npm run build",
    "outputDirectory": "dist/public",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}